# Plot  Angular Distribution

set xlabel "theta"
set ylabel "phi"
set xrange[0:180]
set yrange[0:360]
set hidden3d
set contour base

splot 'teste10degTP.dat' matrix u ($1*5):($2*5):3 w l

pause -1 "enter for image plot"

splot 'teste10degTP.dat' matrix u ($1*5):($2*5):3 w image


pause -1 "enter for Theta plot"
unset yrange
unset xrange

plot [0:180][0:] 'teste10degTheta.dat' u 1:2 w l

pause -1 "enter for Theta plot"
set xlabel "theta"

plot [0:360] 'teste10degPhi.dat' u 1:2 w l

unset xlabel
unset ylabel
unset hidden3d
unset contour



